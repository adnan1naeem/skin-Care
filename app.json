      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "915a5310-84e4-4cee-b154-33c7533d1261"
      },
      "appleId": "6505109083",
      "sku": "com.skincare.EstheMate"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
